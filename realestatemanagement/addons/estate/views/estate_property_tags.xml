<odoo>
    <record id="property_tags_action" model="ir.actions.act_window">
        <field name="name">Property tags</field>
        <field name="res_model">property.tag</field>
          <field name="type">ir.actions.act_window</field>

        <field name="view_mode">form</field>
    </record>

    <record model="ir.ui.view" id="property_tags_view">
        <field name="name">property_tags.view</field>
        <field name="model">property.tag</field>
        <field name="priority" eval="16"> </field>
        <field name="arch" type="xml">
            <form string="property tags">
                <sheet>
                    <h1>Property Tags</h1>
                    <group>
                        <field name="name" />
                    </group>
                </sheet>
            </form>
        </field>

    </record>
</odoo>